div id=\"admin_gonggao\">\r\n    <el-card style=\"height: 100%\">\r\n      <div>\r\n        <el-tabs v-model=\"activeName\" @tab-click=\"handleClick\">\r\n          <!-- ============================公告信息列表====================================== -->\r\n          <el-tab-pane label=\"公告信息列表\" name=\"公告信息列表\" >\r\n            <!-- 公告列表 -->\r\n            <el-button\r\n              type=\"text\"\r\n              @click=\"点击导出表格按钮('gonggaoTable','公告信息列表')\"\r\n              style=\"float: right;\"\r\n            >导出到Ecxel</el-button>\r\n            <el-table\r\n              id=\"gonggaoTable\"\r\n              :data=\"gonggaoTable\"\r\n              border\r\n              stripe\r\n              style=\"width: 100%\"\r\n              max-height=\"700\"\r\n              v-if=\"showTable\"\r\n            >\r\n              <el-table-column prop=\"id\" label=\"#\" width=\"60\"></el-table-column>\r\n              <el-table-column prop=\"title\" label=\"标题\" width=\"270\"></el-table-column>\r\n              <el-table-column prop=\"content\" label=\"内容\"></el-table-column>\r\n              <el-table-column prop=\"type\" label=\"公告类型\" width=\"120\"></el-table-column>\r\n              <el-table-column prop=\"time\" label=\"发布日期\" width=\"140\">\r\n                <template slot-scope=\"scope\">\r\n                  {{scope.row.time|dataTime}}\r\n                </template>\r\n              </el-table-column>\r\n              <el-table-column prop=\"author.nickname\" label=\"作者\" width=\"105\"></el-table-column>\r\n              <el-table-column prop=\"level\" label=\"公告级别\" width=\"80\"></el-table-column>\r\n              <el-table-column align=\"center\" label=\"操作\" width=\"130\">\r\n                <template slot-scope=\"scope\">\r\n                  <el-tooltip class=\"item\" effect=\"dark\" content=\"编辑公告\" placement=\"top\">\r\n                    <el-button\r\n                      type=\"primary\"\r\n                      icon=\"el-icon-edit\"\r\n                      circle\r\n                      @click=\"点击编辑公告按钮(scope.$index)\"\r\n                    ></el-button>\r\n                  </el-tooltip>\r\n                  <el-tooltip class=\"item\" effect=\"dark\" content=\"删除公告\" placement=\"top\">\r\n                    <el-button\r\n                      type=\"danger\"\r\n                      icon=\"el-icon-delete\"\r\n                      circle\r\n                      @click=\"点击删除公告按钮(scope.row.id)\"\r\n                    ></el-button>\r\n                  </el-tooltip>\r\n                </template>\r\n              </el-table-column>\r\n            </el-table>\r\n            <!-- 修改编辑公告模块 -->\r\n            <div v-if=\"!showTable\">\r\n              <div style=\"text-align:center;\">\r\n                <h1 style=\"font-size: 28px;\">编辑修改公告</h1>\r\n              </div>\r\n              <el-row class=\"gonggao\">\r\n                <el-col :span=\"8\" :offset=\"8\">\r\n                  <el-card style=\"    border-radius: 4px;padding-buttom:50px;\">\r\n                    <div slot=\"header\">\r\n                      <el-input v-model=\"tableRowData.title\" placeholder=\"请添加标题\"></el-input>\r\n                    </div>\r\n                    <div>\r\n                      <el-input type=\"textarea\" v-model=\"tableRowData.content\" placeholder=\"请填写内容\"></el-input>\r\n                    </div>\r\n                  </el-card>\r\n                  <div style=\"padding: 10px;\">\r\n                    <span>设置公告级别</span>\r\n                    <el-slider v-model=\"tableRowData.level\" :step=\"10\" show-stops show-input></el-slider>\r\n                  </div>\r\n                  <div style=\"padding: 10px;\">\r\n                    <span style=\"padding-right: 20px;\">设置公告类型</span>\r\n                    <el-select v-model=\"tableRowData.type\" placeholder=\"请选择公告类型\">\r\n                      <el-option label=\"系统公告\" value=\"系统公告\"></el-option>\r\n                      <el-option label=\"荣誉墙\" value=\"荣誉墙\"></el-option>\r\n                      <el-option label=\"竞赛信息\" value=\"竞赛信息\"></el-option>\r\n                    </el-select>\r\n                  </div>\r\n                  <div style=\"text-align: center;margin: 30px;\">\r\n                    <el-button type=\"success\" plain icon=\"el-icon-message\" @click=\"确认编辑公告修改()\">确认修改</el-button>\r\n                    <el-button type=\"warning\" plain icon=\"el-icon-message\" @click=\"取消编辑公告修改()\">取消修改</el-button>\r\n                  </div>\r\n                </el-col>\r\n              </el-row>\r\n            </div>\r\n          </el-tab-pane>\r\n          <!-- ============================发布新公告====================================== -->\r\n          <el-tab-pane label=\"发布新公告\" name=\"发布新公告\" style=\"text-algin:center;\">\r\n            <div style=\"text-align:center;\">\r\n              <h1 style=\"font-size: 28px;\">发布新公告</h1>\r\n            </div>\r\n            <el-row class=\"gonggao\">\r\n              <el-col :span=\"8\" :offset=\"8\">\r\n                <el-card style=\"    border-radius: 4px;padding-buttom:50px;\">\r\n                  <div slot=\"header\">\r\n                    <el-input v-model=\"newGonggao.title\" placeholder=\"请添加标题\"></el-input>\r\n                  </div>\r\n                  <div>\r\n                    <el-input type=\"textarea\" v-model=\"newGonggao.content\" placeholder=\"请填写内容\"></el-input>\r\n                  </div>\r\n                </el-card>\r\n                <div style=\"padding: 10px;\">\r\n                  <span>设置公告级别</span>\r\n                  <el-slider v-model=\"newGonggao.level\" :step=\"10\" show-stops show-input></el-slider>\r\n                </div>\r\n                <div style=\"padding: 10px;\">\r\n                  <span style=\"padding-right: 20px;\">设置公告类型</span>\r\n                  <el-select v-model=\"newGonggao.type\" placeholder=\"请选择公告类型\">\r\n                    <el-option label=\"系统公告\" value=\"系统公告\"></el-option>\r\n                    <el-option label=\"荣誉墙\" value=\"荣誉墙\"></el-option>\r\n                    <el-option label=\"竞赛信息\" value=\"竞赛信息\"></el-option>\r\n                  </el-select>\r\n                </div>\r\n\r\n                <div style=\"text-align: center;margin: 30px;\">\r\n                  <el-button type=\"success\" plain icon=\"el-icon-message\" @click=\"发布公告()\">发布公告</el-button>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </el-tab-pane>\r\n          <!-- ============================信息管理====================================== -->\r\n          <el-tab-pane label=\"信息管理\" name=\"信息管理\">\r\n            <el-tabs v-model=\"activeXinxiName\" :tab-position=\"tabPosition\">\r\n              <el-tab-pane label=\"消息列表\" name=\"消息列表\">\r\n                <el-table border stripe :data=\"xinxiTable\" style=\"width: 100%\">\r\n                  <el-table-column label=\"#\" width=\"60\">\r\n                    <template slot-scope=\"scope\">{{scope.$index + 1}}</template>\r\n                  </el-table-column>\r\n                  <el-table-column prop=\"title\" label=\"标题\" width=\"260\"></el-table-column>\r\n                  <el-table-column prop=\"content\" label=\"内容\"></el-table-column>\r\n                  <el-table-column prop=\"fasongzhe\" label=\"发送者\" width=\"260\"></el-table-column>\r\n                  <el-table-column label=\"时间\" width=\"165\">\r\n                    <template slot-scope=\"scope\">\r\n                      <i class=\"el-icon-time\"></i>\r\n                      <span style=\"margin-left: 10px\">{{ scope.row.time }}</span>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column label=\"操作\" width=\"125\">\r\n                    <template slot-scope=\"scope\">\r\n                      <el-button\r\n                        type=\"primary\"\r\n                        icon=\"el-icon-s-flag\"\r\n                        plain\r\n                        size=\"small\"\r\n                        @click=\"点击标记记录按钮(scope.row.id)\"\r\n                      >标记已读</el-button>\r\n                    </template>\r\n                  </el-table-column>\r\n                </el-table>\r\n              </el-tab-pane>\r\n              <el-tab-pane label=\"已读\" name=\"已读\">\r\n                <el-table border stripe :data=\"xinxiTable\" style=\"width: 100%\">\r\n                  <el-table-column label=\"#\" width=\"60\">\r\n                    <template slot-scope=\"scope\">{{scope.$index + 1}}</template>\r\n                  </el-table-column>\r\n                  <el-table-column prop=\"title\" label=\"标题\" width=\"260\"></el-table-column>\r\n                  <el-table-column prop=\"content\" label=\"内容\"></el-table-column>\r\n                  <el-table-column prop=\"fasongzhe\" label=\"发送者\" width=\"260\"></el-table-column>\r\n                  <el-table-column label=\"时间\" width=\"165\">\r\n                    <template slot-scope=\"scope\">\r\n                      <i class=\"el-icon-time\"></i>\r\n                      <span style=\"margin-left: 10px\">{{ scope.row.time }}</span>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column label=\"操作\" width=\"125\">\r\n                    <template slot-scope=\"scope\">\r\n                      <el-button\r\n                        type=\"danger\"\r\n                        icon=\"el-icon-delete\"\r\n                        plain\r\n                        size=\"small\"\r\n                        @click=\"点击删除信息按钮(scope.row.id)\"\r\n                      >删除信息</el-button>\r\n                    </template>\r\n                  </el-table-column>\r\n                </el-table>\r\n              </el-tab-pane>\r\n            </el-tabs>\r\n          </el-tab-pane>\r\n        </el-tabs>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { dateFormat, exportExcel } from \"../../assets/js/utils.js\";\r\nexport default {\r\n  name: \"admin_gonggao\",\r\n  data() {\r\n    return {\r\n      tabPosition: \"left\",\r\n      showTable: true,\r\n      activeName: \"公告信息列表\",\r\n      activeXinxiName: \"消息列表\",\r\n      tableRowData: {},\r\n      gonggaoTable: [],\r\n      xinxiTable: [],\r\n      newGonggao: {\r\n        title: \"\",\r\n        content: \"\",\r\n        level: 50,\r\n        author: \"\",\r\n        type: \"\"\r\n      }\r\n    };\r\n  },\r\n  mounted() {\r\n    this.请求公告信息列表数据();\r\n  },\r\n  methods: {\r\n    handleClick(tab, event) {\r\n      //   console.log(tab, event);\r\n    },\r\n    // getRowData(row){\r\n    //     this.tableRowData = JSON.parse(JSON.stringify(row)); // 先转换为字符串，然后再转换\r\n    // },\r\n    点击编辑公告按钮(index) {\r\n      this.showTable = false;\r\n      this.tableRowData = this.gonggaoTable[index];\r\n    },\r\n    点击删除公告按钮(id) {\r\n      let that = this;\r\n      this.$confirm(\"此操作将永久删除该公告, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\"\r\n      })\r\n        .then(() => {\r\n          this.$axios\r\n            .get(`http://localhost:8089/notice/deleteNoticeById?id=${id}`)\r\n            .then(function(response) {\r\n              that.$message({\r\n                message: response.data,\r\n                type: response.data == \"删除成功！\" ? \"success\" : \"error\"\r\n              });\r\n              if (response.data == \"删除成功！\") {\r\n                that.请求公告信息列表数据();\r\n              }\r\n            })\r\n            .catch(function(error) {\r\n              that.$message.error(\"服务器错误，操作失败！\");\r\n              console.error(error);\r\n            });\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消删除\"\r\n          });\r\n        });\r\n    },\r\n    取消编辑公告修改() {\r\n      let that = this;\r\n      this.$confirm(\"此操作将放弃对公告的修改, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\"\r\n      })\r\n        .then(() => {\r\n          //确定\r\n          that.tableRowData = {};\r\n          that.showTable = true;\r\n        })\r\n        .catch(() => {\r\n          //取消\r\n          return;\r\n        });\r\n    },\r\n    确认编辑公告修改() {\r\n      if (this.tableRowData.title == \"\" || this.tableRowData.content == \"\") {\r\n        this.$message.error(\"标题和内容不能为空！\");\r\n        return;\r\n      }\r\n      let that = this;\r\n      //创建表单对象\r\n      let gonggao = new FormData();\r\n      gonggao.append(\"id\", that.tableRowData.id);\r\n      gonggao.append(\"title\", that.tableRowData.title);\r\n      gonggao.append(\"content\", that.tableRowData.content);\r\n      gonggao.append(\"level\", that.tableRowData.level);\r\n      gonggao.append(\"author.id\", that.$store.state.id);\r\n      gonggao.append(\"type\", that.tableRowData.type);\r\n\r\n      this.$axios\r\n        .post(`http://localhost:8089/notice/updateNotice`, gonggao)\r\n        .then(function(response) {\r\n          console.log(response.data);\r\n          that.$message({\r\n            message: response.data == \"发布成功！\" ?'修改成功':response.data,\r\n            type: response.data == \"发布成功！\" ? \"success\" : \"error\"\r\n          });\r\n          that.showTable = true;\r\n        })\r\n        .catch(function(error) {\r\n          that.$message.error(\"服务器错误，操作失败！\");\r\n          console.error(error);\r\n        });\r\n    },\r\n    发布公告() {\r\n      if (this.newGonggao.title == \"\" || this.newGonggao.content == \"\") {\r\n        this.$message.error(\"标题和内容不能为空！\");\r\n        return;\r\n      }\r\n\r\n      let that = this;\r\n      let date = new Date();\r\n      //创建表单对象\r\n      let gonggao = new FormData();\r\n      gonggao.append(\"title\", that.newGonggao.title);\r\n      gonggao.append(\"content\", that.newGonggao.content);\r\n      gonggao.append(\"level\", that.newGonggao.level);\r\n      gonggao.append(\"author.id\", that.$store.state.id);\r\n      gonggao.append(\"type\", that.newGonggao.type);\r\n\r\n      this.$axios\r\n        .post(`http://localhost:8089/notice/addNotice`, gonggao)\r\n        .then(function(response) {\r\n          that.$message({\r\n            message: response.data,\r\n            type: response.data == \"发布成功！\" ? \"success\" : \"error\"\r\n          });\r\n          that.newGonggao.title = \"\";\r\n          that.newGonggao.content = \"\";\r\n          that.activeName = \"公告信息列表\";\r\n        })\r\n        .catch(function(error) {\r\n          that.$message.error(\"服务器错误，操作失败！\");\r\n          console.error(error);\r\n        });\r\n    },\r\n\r\n    请求公告信息列表数据() {\r\n      let that = this;\r\n      this.$axios\r\n        .get(`http://localhost:8089/notice/findAllNotice`)\r\n        .then(function(response) {\r\n          // console.log(response);\r\n          that.gonggaoTable = response.data;\r\n        })\r\n        .catch(function(error) {\r\n          that.$message.error(\"服务器错误，操作失败！\");\r\n          console.error(error);\r\n        });\r\n    },\r\n    点击导出表格按钮(tableDOM, excelName) {\r\n      exportExcel(tableDOM, excelName);\r\n    },\r\n    获取未读消息列表() {\r\n      let that = this;\r\n      that.$axios\r\n        .get(\r\n          `http://localhost:8088/xinxi/selectXinxiByJieshouzheAndYidu/?jieshouzhe=AllAdmin&yidu=0`\r\n        )\r\n        .then(function(response) {\r\n          that.xinxiTable = response.data;\r\n          // console.log(response.data[0]);\r\n        })\r\n        .catch(function(error) {\r\n          that.$message.error(\"服务器错误，操作失败！\");\r\n          console.log(error);\r\n        });\r\n    },\r\n    获取已读消息列表() {\r\n      let that = this;\r\n      that.$axios\r\n        .get(\r\n          `http://localhost:8088/xinxi/selectXinxiByJieshouzheAndYidu/?jieshouzhe=AllAdmin&yidu=1`\r\n        )\r\n        .then(function(response) {\r\n          that.xinxiTable = response.data;\r\n          // console.log(response.data[0]);\r\n        })\r\n        .catch(function(error) {\r\n          that.$message.error(\"服务器错误，操作失败！\");\r\n          console.log(error);\r\n        });\r\n    },\r\n    点击标记记录按钮(xinxiId) {\r\n      let that = this;\r\n      that.$axios\r\n        .get(\r\n          `http://localhost:8088/xinxi/updateXinxiYiduById/?id=${xinxiId}&yidu=1`\r\n        )\r\n        .then(function(response) {\r\n          if (response.data == \"error\") {\r\n            that.$message.error(\"服务器错误，操作失败！\");\r\n          } else {\r\n            that.$message({\r\n              type: \"success\",\r\n              message: \"信息已放入到已读页面!\"\r\n            });\r\n            that.获取未读消息列表();\r\n          }\r\n        })\r\n        .catch(function(error) {\r\n          that.$message.error(\"服务器错误，操作失败！\");\r\n          console.log(error);\r\n        });\r\n    },\r\n    点击删除信息按钮(xinxiId) {\r\n      this.$confirm(\"此操作将永久删除该信息, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\"\r\n      })\r\n        .then(() => {\r\n          let that = this;\r\n          that.$axios\r\n            .get(`http://localhost:8088/xinxi/deleteXinxiById/?id=${xinxiId}`)\r\n            .then(function(response) {\r\n              if (response.data == \"error\") {\r\n                that.$message.error(\"服务器错误，操作失败！\");\r\n              } else {\r\n                that.$message({\r\n                  type: \"success\",\r\n                  message: \"删除成功!\"\r\n                });\r\n                that.获取已读消息列表();\r\n              }\r\n            })\r\n            .catch(function(error) {\r\n              that.$message.error(\"服务器错误，操作失败！\");\r\n              console.log(error);\r\n            });\r\n        })\r\n        .catch(() => {\r\n          that.$message({\r\n            type: \"info\",\r\n            message: \"已取消删除\"\r\n          });\r\n        });\r\n    }\r\n  },\r\n  watch: {\r\n    activeName(val, oldVal) {\r\n      let that = this;\r\n      that.tableRowData = {};\r\n      that.showTable = true;\r\n      if (that.activeName == \"公告信息列表\") {\r\n        that.请求公告信息列表数据();\r\n      } else if (that.activeName == \"信息管理\") {\r\n        if (that.activeXinxiName != \"消息列表\") {\r\n          that.activeXinxiName = \"消息列表\";\r\n        } else {\r\n          that.获取未读消息列表();\r\n        }\r\n      }\r\n    },\r\n    activeXinxiName(val, oldVal) {\r\n      let that = this;\r\n      if (that.activeXinxiName == \"消息列表\") {\r\n        that.获取未读消息列表();\r\n      } else if (that.activeXinxiName == \"已读\") {\r\n        that.获取已读消息列表();\r\n      }\r\n    }\r\n  },\r\n  filters:{\r\n    dataTime: function(value) {\r\n      if (value==undefined)\r\n        return '-';\r\n      value = new Date(value)\r\n      if (value instanceof Date) {\r\n        var d = value;\r\n        var year=d.getFullYear();\r\n        var m = d.getMonth() + 1;\r\n        var day = d.getDate() < 10 ? '0' + d.getDate() : d.getDate();\r\n        var myDate = year + '年'+m + '月 ' + day +'日';\r\n        return myDate;\r\n      } else {\r\n        return \"-\";\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style>\r\n.gonggao .el-card input.el-input__inner {\r\n  height: 65px;\r\n  font-size: 24px;\r\n}\r\n.gonggao .el-card__header {\r\n  padding: 0;\r\n}\r\n.gonggao .el-card__content {\r\n  padding: 0;\r\n}\r\n.gonggao textarea.el-textarea__inner {\r\n  height: 330px;\r\n}\r\n</style>"],"sourceRoot":"src/views/admin"}]}