suoshuxueke: \"\",\r\n        didian: \"\",\r\n        projectName: \"\",\r\n        zhidaolaoshi: \"\",\r\n        suoshuxueyuan: \"\",\r\n        zhuangtai: \"已审核\"\r\n      },\r\n      dialog: { studentInfo: {}, projectInfo: {}, xinxi: {} }\r\n    };\r\n  },\r\n  mounted() {\r\n    this.获取项目和竞赛的联合信息();\r\n  },\r\n  methods: {\r\n    获取项目和竞赛的联合信息() {\r\n      let that = this;\r\n      that.$axios\r\n        .get(\r\n          `http://localhost:8088/project/relationAllProAndCompByZt/?zhuangtai=已审核`\r\n        )\r\n        .then(function(response) {\r\n          that.tableData = response.data;\r\n        })\r\n        .catch(function(error) {\r\n          console.log(error);\r\n        });\r\n    },\r\n    点击综合查询选项查询按钮() {\r\n      let that = this;\r\n      that.$axios\r\n        .post(\r\n          `http://localhost:8088/project/LikeRelationAllProAndCompByZt`,\r\n          this.searchFormData\r\n        )\r\n        .then(function(response) {\r\n          that.tableData = response.data;\r\n        })\r\n        .catch(function(error) {\r\n          console.log(error);\r\n        });\r\n    },\r\n    查看全部() {\r\n      this.searchFormData = {};\r\n      this.获取项目和竞赛的联合信息();\r\n    },\r\n    点击申请按钮(row) {\r\n      this.dialogVisible = true;\r\n      this.dialog.projectInfo = row;\r\n      this.dialog.studentInfo = this.$parent.$parent.$parent.$parent.studentInfo;\r\n    },\r\n    点击申请参加按钮() {\r\n      let that = this;\r\n      if (that.dialog.message == \"\") {\r\n        that.$message({\r\n          message: \"请填写申请信息！\",\r\n          type: \"warning\"\r\n        });\r\n        return;\r\n      }\r\n      that.$axios\r\n        .post(`http://localhost:8088/student/joinProject`, that.dialog)\r\n        .then(function(response) {\r\n          if (response.data == \"error\") {\r\n            that.$message.error(\"服务器错误，请稍后再试！\");\r\n          } else {\r\n            that.$message({\r\n              message: response.data,\r\n              type: \"success\"\r\n            });\r\n          }\r\n          that.dialog.message = \"\";\r\n        })\r\n        .catch(function(error) {\r\n          console.log(error);\r\n        });\r\n\r\n      that.dialogVisible = false;\r\n    },\r\n    handleClose() {\r\n      let that = this;\r\n      this.$confirm(\"确认关闭？\")\r\n        .then(_ => {\r\n          that.dialog.xinxi = \"\";\r\n          that.dialogVisible = false;\r\n        })\r\n        .catch(_ => {});\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style>\r\n#joinProDialog .el-col.el-col-8 {\r\n  font-size: 18px;\r\n  font-weight: 600;\r\n}\r\n#joinProDialog .el-row {\r\n  margin: 10px;\r\n}\r\n\r\n#joinProDialog textarea.el-textarea__inner {\r\n  height: 200px;\r\n}\r\n</style>"],"sourceRoot":"src/views/student"}]}