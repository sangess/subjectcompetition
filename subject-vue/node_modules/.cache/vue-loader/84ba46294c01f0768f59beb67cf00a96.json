    <el-form ref=\"searchFormData\" :model=\"searchFormData\" label-width=\"80px\">\r\n            <el-col :span=\"4\">\r\n              <el-form-item label=\"竞赛名称\" prop=\"name\">\r\n                <el-input v-model=\"searchFormData.name\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span=\"4\">\r\n              <el-form-item label=\"主办单位\" prop=\"zhubandanwei\">\r\n                <el-input v-model=\"searchFormData.zhubandanwei\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span=\"4\">\r\n              <el-form-item label=\"竞赛级别\" prop=\"jibie\">\r\n                <el-select v-model=\"searchFormData.jibie\" placeholder=\"请选择竞赛级别\">\r\n                  <el-option label=\"国家级\" value=\"国家级\"></el-option>\r\n                  <el-option label=\"省级\" value=\"省级\"></el-option>\r\n                  <el-option label=\"校级\" value=\"校级\"></el-option>\r\n                  <el-option label value></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span=\"4\">\r\n              <el-form-item label=\"所属学科\" prop=\"suoshuxueke\">\r\n                <el-input v-model=\"searchFormData.suoshuxueke\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span=\"4\">\r\n              <el-form-item label=\"竞赛地点\" prop=\"didian\">\r\n                <el-input v-model=\"searchFormData.didian\"></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span=\"4\">\r\n              <el-form-item label=\"竞赛状态\" prop=\"zhuangtai\">\r\n                <el-select v-model=\"searchFormData.zhuangtai\" placeholder=\"请选择竞赛状态\">\r\n                  <el-option label=\"未开始\" value=\"未开始\"></el-option>\r\n                  <el-option label=\"进行中\" value=\"进行中\"></el-option>\r\n                  <el-option label=\"已结束\" value=\"已结束\"></el-option>\r\n                  <el-option label value></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-form>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span=\"3\" :offset=\"21\">\r\n            <el-button\r\n              type=\"primary\"\r\n              size=\"medium\"\r\n              icon=\"el-icon-search\"\r\n              style=\"    float: right;\"\r\n              @click=\"点击综合查询选项查询按钮()\"\r\n            >筛选 / 查询</el-button>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n    </el-card>\r\n    <el-card class=\"box-card\">\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <span>竞赛列表</span>\r\n        <el-button\r\n          type=\"primary\"\r\n          size=\"medium\"\r\n          icon=\"el-icon-circle-plus-outline\"\r\n          style=\"float: right;\"\r\n          @click=\"添加新竞赛()\"\r\n        >添加新竞赛</el-button>\r\n      </div>\r\n\r\n      <el-table\r\n        v-loading=\"loading\"\r\n        :data=\"tableData.slice((currentPage-1)*pagesize,currentPage*pagesize)\"\r\n        stripe\r\n        border\r\n        height=\"440px\"\r\n        style=\"width: 100%；\"\r\n        class=\"elTable\"\r\n        @row-click=\"getDetails\"\r\n      >\r\n        <el-table-column align=\"center\" prop=\"id\" label=\"#\" width=\"50px\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"name\" label=\"竞赛名称\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"zhubandanwei\" label=\"主办单位\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"jibie\" label=\"级 别\" width=\"120px\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"suoshuxueke\" label=\"所属学科\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"shijian\" label=\"竞赛时间\" width=\"130px\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"didian\" label=\"竞赛地点\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"竞赛项目\" label=\"竞赛项目\" width=\"120px\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"text\" @click=\"查看项目列表(scope)\">点击查看</el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column align=\"center\" prop=\"zhuangtai\" label=\"状态\" width=\"80px\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag\r\n              :type=\"scope.row.zhuangtai === '进行中' ? 'success' : (scope.row.zhuangtai === '未开始' ? 'primary':'warning')\"\r\n              disable-transitions\r\n            >{{scope.row.zhuangtai}}</el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column align=\"center\" label=\"操作\" width=\"80px\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tooltip class=\"item\" effect=\"dark\" content=\"编辑\" placement=\"left\">\r\n              <el-button icon=\"el-icon-edit\" circle @click=\"编辑(scope.$index)\"></el-button>\r\n            </el-tooltip>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <div id=\"fenye\">\r\n        <!-- 设置layout，表示需要显示的内容，用逗号分隔，布局元素会依次显示。\r\n          prev表示上一页，next为下一页，pager表示页码列表，\r\n          除此以外还提供了jumper和total，size和特殊的布局符号->，->后的元素会靠右显示，\r\n          jumper表示跳页元素，\r\n          total表示总条目数，\r\n          page-size  每页多少条数据\r\n          @current-change=\"current_change\" //currentPage改变时会触发\r\n        size用于设置每页显示的页码数量。-->\r\n        <el-pagination\r\n          background\r\n          layout=\"prev, pager, next\"\r\n          :total=\"tableData.length\"\r\n          :page-size=\"12\"\r\n          @current-change=\"current_change\"\r\n          :current-page.sync=\"currentPage\"\r\n        ></el-pagination>\r\n      </div>\r\n    </el-card>\r\n    <!-- 竞赛项目展示模态框 -->\r\n    <el-dialog title=\"竞赛项目列表\" :visible.sync=\"projectVisible\">\r\n      <el-table :data=\"projectData\" stripe border style=\"width: 100%\">\r\n        <el-table-column align=\"center\" prop=\"id\" label=\"项目ID\" width=\"80px\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"name\" label=\"项目名称\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"suoshuxueyuan\" label=\"所属学院\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"zhidaolaoshi\" label=\"指导老师\" width=\"120px\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"yusuan\" label=\"预算\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"zhuangtai\" label=\"状态\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag\r\n              :type=\"scope.row.zhuangtai === '已审核' ? 'success' : 'warning'\"\r\n              disable-transitions\r\n            >{{scope.row.zhuangtai}}</el-tag>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n    <!-- 修改信息模态框 -->\r\n    <el-dialog :visible.sync=\"dialogFormVisible\" :before-close=\"handleClose\">\r\n      <el-form :model=\"rowData\" ref=\"form\">\r\n        <el-form-item label=\"竞赛名称\">\r\n          <el-input v-model=\"rowData.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"主办单位\">\r\n          <el-input v-model=\"rowData.zhubandanwei\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"竞赛地点\">\r\n          <el-input v-model=\"rowData.didian\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"所属学科\">\r\n          <el-input v-model=\"rowData.suoshuxueke\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"竞赛时间\">\r\n          <el-date-picker\r\n            v-model=\"rowData.shijian\"\r\n            type=\"date\"\r\n            placeholder=\"选择日期\"\r\n            value-format=\"yyyy-MM-dd\"\r\n            format=\"yyyy 年 MM 月 dd 日\"\r\n          ></el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"竞赛级别\">\r\n          <el-select v-model=\"rowData.jibie\" placeholder>\r\n            <el-option label=\"国家级\" value=\"国家级\"></el-option>\r\n            <el-option label=\"省级\" value=\"省级\"></el-option>\r\n            <el-option label=\"校级\" value=\"校级\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"竞赛状态\">\r\n          <el-select v-model=\"rowData.zhuangtai\" placeholder>\r\n            <el-option label=\"未开始\" value=\"未开始\"></el-option>\r\n            <el-option label=\"进行中\" value=\"进行中\"></el-option>\r\n            <el-option label=\"已结束\" value=\"已结束\"></el-option>\r\n            <!-- <el-input v-model=\"rowData.状态\" autocomplete=\"off\"></el-input> -->\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"修改提交()\">确定修改</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n<style>\r\n.clearfix {\r\n  font-size: 26px;\r\n}\r\n#fenye {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n<script>\r\nexport default {\r\n  name: \"admin_comp\",\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      pagesize: 12,\r\n      currentPage: 1,\r\n      dialogFormVisible: false,\r\n      projectVisible: false,\r\n      projectData: {},\r\n      rowIndex: -1,\r\n      rowData: {\r\n        id: 0,\r\n        name: \"\",\r\n        zhubandanwei: \"\",\r\n        jibie: \"\",\r\n        suoshuxueke: \"\",\r\n        shijian: \"\",\r\n        didian: \"\",\r\n        zhuangtai: \"\"\r\n      },\r\n      tableData: [],\r\n      searchFormData: {\r\n        name: \"\",\r\n        zhubandanwei: \"\",\r\n        jibie: \"\",\r\n        suoshuxueke: \"\",\r\n        didian: \"\",\r\n        zhuangtai: \"\"\r\n      }\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    var that = this;\r\n    console.log(\"========请求数据=========\");\r\n    that.loading = true;\r\n    this.$axios\r\n      .post(\"http://localhost:8088/comp/selectAllComp\")\r\n      .then(function(response) {\r\n        that.tableData = response.data;\r\n        that.loading = false;\r\n        // that.$store.commit(\"setTableData\", that.tableData);\r\n      })\r\n      .catch(function(error) {\r\n        console.log(error);\r\n        that.loading = false;\r\n      });\r\n  },\r\n  methods: {\r\n    点击综合查询选项查询按钮() {\r\n      let that = this;\r\n      let searchData = new FormData();\r\n      searchData.append(\"name\", that.searchFormData.name);\r\n      searchData.append(\"zhubandanwei\", that.searchFormData.zhubandanwei);\r\n      searchData.append(\"jibie\", that.searchFormData.jibie);\r\n      searchData.append(\"suoshuxueke\", that.searchFormData.suoshuxueke);\r\n      searchData.append(\"didian\", that.searchFormData.didian);\r\n      searchData.append(\"zhuangtai\", that.searchFormData.zhuangtai);\r\n      this.$axios\r\n        .post(\"http://localhost:8088/comp/fuzzySearch\", searchData)\r\n        .then(function(response) {\r\n          that.tableData = response.data;\r\n        })\r\n        .catch(function(error) {\r\n          that.$message.error(\"系统错误，操作失败，请稍后再试!\");\r\n        });\r\n    },\r\n\r\n    getDetails(row) {\r\n      // this.rowData = row;\r\n      this.rowData = JSON.parse(JSON.stringify(row)); // 先转换为字符串，然后再转换\r\n    },\r\n    编辑(index) {\r\n      this.dialogFormVisible = true;\r\n      this.rowIndex = index;\r\n    },\r\n    修改提交() {\r\n      let that = this;\r\n      let comp = new FormData();\r\n      comp.append(\"id\", that.rowData.id);\r\n      comp.append(\"name\", that.rowData.name);\r\n      comp.append(\"zhubandanwei\", that.rowData.zhubandanwei);\r\n      comp.append(\"jibie\", that.rowData.jibie);\r\n      comp.append(\"suoshuxueke\", that.rowData.suoshuxueke);\r\n      comp.append(\"shijian\", that.rowData.shijian);\r\n      comp.append(\"didian\", that.rowData.didian);\r\n      comp.append(\"zhuangtai\", that.rowData.zhuangtai);\r\n\r\n      this.$confirm(\"确认修改？\")\r\n        .then(_ => {\r\n          this.$axios\r\n            .post(\"http://localhost:8088/comp/updateComp\", comp)\r\n            .then(function(response) {\r\n              that.$set(that.tableData, that.rowData.id - 1, that.rowData);\r\n\r\n              //提示信息\r\n              that.$message({\r\n                message: response.data,\r\n                type: \"success\"\r\n              });\r\n              //模态框关闭\r\n              that.dialogFormVisible = false;\r\n            })\r\n            .catch(function(error) {\r\n              that.$message.error(error);\r\n              console.log(error);\r\n            });\r\n\r\n          done();\r\n        })\r\n        .catch(_ => {});\r\n    },\r\n    查看项目列表(scope) {\r\n      // console.log(scope.row.id);\r\n\r\n      var that = this;\r\n      that.compName = scope.row.name;\r\n      that.projectVisible = true;\r\n      this.$axios\r\n        .get(\r\n          `http://localhost:8088/project/selectProjectByCompId?compId=${scope.row.id}`\r\n        )\r\n        .then(function(response) {\r\n          that.projectData = response.data;\r\n        })\r\n        .catch(function(error) {\r\n          console.log(error);\r\n        });\r\n    },\r\n    添加新竞赛() {\r\n      this.$router.push(`/admin/comp/add`);\r\n    },\r\n    current_change(currentPage) {\r\n      //改变当前页\r\n      this.currentPage = currentPage;\r\n    },\r\n    handleClose(done) {\r\n      this.$confirm(\"确认关闭？\")\r\n        .then(_ => {\r\n          done();\r\n        })\r\n        .catch(_ => {});\r\n    }\r\n  },\r\n  computed: {}\r\n};\r\n</script>"]}]}