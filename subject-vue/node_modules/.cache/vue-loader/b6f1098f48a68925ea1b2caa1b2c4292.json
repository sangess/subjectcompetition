scope.row)\">点击申请</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n    </el-card>\r\n\r\n    <!-- 申请参赛模态框 -->\r\n    <el-dialog\r\n      title=\"信息确认\"\r\n      :visible.sync=\"dialogVisible\"\r\n      :close-on-click-modal=\"false\"\r\n      :close-on-press-escape=\"false\"\r\n      :show-close=\"false\"\r\n      width=\"30%\"\r\n      id=\"joinProDialog\"\r\n    >\r\n      <el-row :gutter=\"10\">\r\n        <el-col :span=\"12\">\r\n          <el-row :gutter=\"10\">\r\n            <el-col :span=\"8\">\r\n              <span class=\"el-icon-user-solid\"></span> 姓名：\r\n            </el-col>\r\n            <el-col :span=\"14\">\r\n              <span\r\n                style=\"color: teal;font-style: italic;font-size: 16px;\"\r\n              >{{dialog.studentInfo.name}}</span>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row :gutter=\"10\">\r\n            <el-col :span=\"8\">\r\n              <span class=\"el-icon-s-help\"></span> 学号：\r\n            </el-col>\r\n            <el-col :span=\"14\">\r\n              <span\r\n                style=\"color: teal;font-style: italic;font-size: 16px;\"\r\n              >{{dialog.studentInfo.studentNumber}}</span>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row :gutter=\"10\">\r\n            <el-col :span=\"8\">\r\n              <span class=\"el-icon-s-promotion\"></span> 年龄：\r\n            </el-col>\r\n            <el-col :span=\"14\">\r\n              <span\r\n                style=\"color: teal; font-style: italic; font-size: 16px;\"\r\n              >{{dialog.studentInfo.age}}</span>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row :gutter=\"10\">\r\n            <el-col :span=\"8\">\r\n              <span class=\"el-icon-s-opportunity\"></span> 性别：\r\n            </el-col>\r\n            <el-col :span=\"14\">\r\n              <span\r\n                style=\"color: teal; font-style: italic; font-size: 16px;\"\r\n              >{{dialog.studentInfo.sex}}</span>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row :gutter=\"10\">\r\n            <el-col :span=\"8\">\r\n              <span class=\"el-icon-s-open\"></span> 专业：\r\n            </el-col>\r\n            <el-col :span=\"14\">\r\n              <span\r\n                style=\"color: teal; font-style: italic; font-size: 16px;\"\r\n              >{{dialog.studentInfo.zhuanye}}</span>\r\n            </el-col>\r\n          </el-row>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <el-row :gutter=\"10\">\r\n            <el-col :span=\"8\">\r\n              <span class=\"el-icon-s-order\"></span> 竞赛：\r\n            </el-col>\r\n            <el-col :span=\"14\">\r\n              <span\r\n                style=\"color: teal;font-style: italic;font-size: 16px;\"\r\n              >{{dialog.projectInfo.compName}}</span>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row :gutter=\"10\">\r\n            <el-col :span=\"8\">\r\n              <span class=\"el-icon-s-management\"></span> 项目：\r\n            </el-col>\r\n            <el-col :span=\"14\">\r\n              <span\r\n                style=\"color: teal;font-style: italic;font-size: 16px;\"\r\n              >{{dialog.projectInfo.name}}</span>\r\n            </el-col>\r\n          </el-row>\r\n\r\n          <el-row :gutter=\"10\">\r\n            <el-col :span=\"8\">\r\n              <span class=\"el-icon-s-finance\"></span> 级别：\r\n            </el-col>\r\n            <el-col :span=\"14\">\r\n              <span\r\n                style=\"color: teal;font-style: italic;font-size: 16px;\"\r\n              >{{dialog.projectInfo.jibie}}</span>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row :gutter=\"10\">\r\n            <el-col :span=\"8\">\r\n              <span class=\"el-icon-s-promotion\"></span> 地点：\r\n            </el-col>\r\n            <el-col :span=\"14\">\r\n              <span\r\n                style=\"color: teal;font-style: italic;font-size: 16px;\"\r\n              >{{dialog.projectInfo.didian}}</span>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row :gutter=\"10\">\r\n            <el-col :span=\"8\">\r\n              <span class=\"el-icon-s-check\"></span> 时间：\r\n            </el-col>\r\n            <el-col :span=\"14\">\r\n              <span\r\n                style=\"color: teal;font-style: italic;font-size: 16px;\"\r\n              >{{dialog.projectInfo.shijian}}</span>\r\n            </el-col>\r\n          </el-row>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"10\">\r\n        <el-input type=\"textarea\" placeholder=\"请填写申请信息\" v-model=\"dialog.message\"></el-input>\r\n      </el-row>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"handleClose()\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"点击申请参加按钮()\">申请参加</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"joinProject\",\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n      tableData: [],\r\n      searchFormData: {\r\n        compName: \"\",\r\n        zhubandanwei: \"\",\r\n        jibie: \"\",\r\n        suoshuxueke: \"\",\r\n        didian: \"\",\r\n        projectName: \"\",\r\n        zhidaolaoshi: \"\",\r\n        suoshuxueyuan: \"\",\r\n        zhuangtai: \"已审核\"\r\n      },\r\n      dialog: { studentInfo: {}, projectInfo: {}, xinxi: {} }\r\n    };\r\n  },\r\n  mounted() {\r\n    this.获取项目和竞赛的联合信息();\r\n  },\r\n  methods: {\r\n    获取项目和竞赛的联合信息() {\r\n      let that = this;\r\n      that.$axios\r\n        .get(\r\n          `http://localhost:8088/project/relationAllProAndCompByZt/?zhuangtai=已审核`\r\n        )\r\n        .then(function(response) {\r\n          that.tableData = response.data;\r\n        })\r\n        .catch(function(error) {\r\n          console.log(error);\r\n        });\r\n    },\r\n    点击综合查询选项查询按钮() {\r\n      let that = this;\r\n      that.$axios\r\n        .post(\r\n          `http://localhost:8088/project/LikeRelationAllProAndCompByZt`,\r\n          this.searchFormData\r\n        )\r\n        .then(function(response) {\r\n          that.tableData = response.data;\r\n        })\r\n        .catch(function(error) {\r\n          console.log(error);\r\n        });\r\n    },\r\n    查看全部() {\r\n      this.searchFormData = {};\r\n      this.获取项目和竞赛的联合信息();\r\n    },\r\n    点击申请按钮(row) {\r\n      this.dialogVisible = true;\r\n      this.dialog.projectInfo = row;\r\n      this.dialog.studentInfo = this.$parent.$parent.$parent.$parent.studentInfo;\r\n    },\r\n    点击申请参加按钮() {\r\n      let that = this;\r\n      if (that.dialog.message == \"\") {\r\n        that.$message({\r\n          message: \"请填写申请信息！\",\r\n          type: \"warning\"\r\n        });\r\n        return;\r\n      }\r\n      that.$axios\r\n        .post(`http://localhost:8088/student/joinProject`, that.dialog)\r\n        .then(function(response) {\r\n          if (response.data == \"error\") {\r\n            that.$message.error(\"服务器错误，请稍后再试！\");\r\n          } else {\r\n            that.$message({\r\n              message: response.data,\r\n              type: \"success\"\r\n            });\r\n          }\r\n          that.dialog.message = \"\";\r\n        })\r\n        .catch(function(error) {\r\n          console.log(error);\r\n        });\r\n\r\n      that.dialogVisible = false;\r\n    },\r\n    handleClose() {\r\n      let that = this;\r\n      this.$confirm(\"确认关闭？\")\r\n        .then(_ => {\r\n          that.dialog.xinxi = \"\";\r\n          that.dialogVisible = false;\r\n        })\r\n        .catch(_ => {});\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style>\r\n#joinProDialog .el-col.el-col-8 {\r\n  font-size: 18px;\r\n  font-weight: 600;\r\n}\r\n#joinProDialog .el-row {\r\n  margin: 10px;\r\n}\r\n\r\n#joinProDialog textarea.el-textarea__inner {\r\n  height: 200px;\r\n}\r\n</style>"]}]}